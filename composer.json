{
  "name": "mecsu/yii2-blog",
  "description": "Publications manager",
  "keywords": ["yii2", "yii2-blog", "blog"],
  "type": "yii2-extension",
  "license": "MIT",
  "version": "1.2.4",
  "authors": [
    {
      "name": "Xuan Nguyen",
      "email": "xuanhn.vt@gmail.com"
    }
  ],
  "repositories":[
    {
        "type": "git",
        "url": "https://github.com/mecsu/yii2-blog"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.40",
    "wdmg/yii2-base": "^1.3.0",
    "wdmg/yii2-selectinput": "^1.0.9",
    "wdmg/yii2-tagsinput": "^1.0.4",
    "wdmg/yii2-helpers": "^1.4.2",
    "wdmg/yii2-editor": "^1.0.10",
    "wdmg/yii2-validators": "^1.0.6",
    "wdmg/yii2-widgets": "^1.0.4"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "mecsu\\blog\\": ""
    }
  },
  "extra": {
    "options": {
      "routePrefix": "admin",
      "baseRoute": "/blog",
      "baseLayout": "@app/views/layouts/main",
      "imagePath": "/uploads/blog",
      "supportLocales": ["ru-RU", "uk-UA", "en-US"]
    }
  }
}